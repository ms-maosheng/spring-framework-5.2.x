<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
 				 http://www.springframework.org/schema/context https://www.springframework.org/schema/context/spring-context.xsd">

	<!--<context:component-scan base-package="ms.springframework.config.populateBean.autowired"/>-->

	<bean id="persion1" class="ms.springframework.config.populateBean.Person" autowire="byName"/>
	<bean id="persion2" class="ms.springframework.config.populateBean.Person" autowire="byType"/>


	<bean id="address" class="ms.springframework.config.populateBean.Address">
		<property name="province" value="浙江"></property>
		<property name="city" value="杭州"></property>
		<property name="area" value="西湖"></property>
	</bean>
</beans>